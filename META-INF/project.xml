            <value>push (@::gMatchers,
    {
      id      =&gt; &quot;deployReport&quot;,
      pattern =&gt; q{Report ready at (.*)},
      action  =&gt; q{	setProperty(&quot;/myJob/report-urls/deployReport&quot;, &quot;$1&quot;);
			setProperty(&quot;summary&quot;, &quot;&lt;html&gt;&lt;b&gt;&lt;a href=\&quot;$1\&quot;&gt;Deploy Report&lt;/a&gt;&lt;/b&gt;&lt;/html&gt;&quot;);
